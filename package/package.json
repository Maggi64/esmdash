{
  "name": "moderndash",
  "version": "2.1.0",
  "type": "module",
  "description": "A lodash inspired utility framework for ESM/Typescript/ESNext",
  "engines": {
    "node": ">=19.x.x",
    "npm": ">=8.x.x"
  },
  "sideEffects": false,
  "scripts": {
    "build:index": "ctix create --startAt src --overwrite --noBackup",
    "build": "tsup",
    "prepublishOnly": "npm run test && npm run build && cp '../README.md' './README.md' && cp '../LICENSE' './LICENSE'",
    "postpublish": "rm './README.md' && rm './LICENSE'",
    "test-dev": "vitest --ui",
    "test": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maggi64/moderndash.git"
  },
  "keywords": [
    "lodash",
    "utility",
    "helper",
    "underscore",
    "esm",
    "typescript"
  ],
  "author": "Maggi64",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Maggi64/moderndash/issues"
  },
  "browserslist": [
    ">2% and not dead"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://moderndash.io",
  "devDependencies": {
    "@vitest/coverage-c8": "0.29.1",
    "@vitest/ui": "0.29.1",
    "vitest": "0.29.1",
    "tsup": "6.6.3",
    "ctix": "1.8.2"
  }
}
